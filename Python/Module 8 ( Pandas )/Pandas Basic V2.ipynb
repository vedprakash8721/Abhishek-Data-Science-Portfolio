    "import pandas as pd\n",
    "@pd.api.extensions.register_series_accessor(\"shout\")\n",
    "class ShoutAccessor:\n",
    "    def __init__(self, pandas_obj):\n",
    "        self._obj = pandas_obj\n",
    "    def upper(self):\n",
    "        return self._obj.str.upper()\n",
    "\n",
    "s = pd.Series([\"hello\", \"ved\"])\n",
    "print(s.shout.upper())\n"
   ]
  }
